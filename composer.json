{
  "name": "acquia/blt",
  "description": "BLT",
  "license": "GPL-2.0",
  "require": {
    "drupal/console":                   "~1",
    "phing/phing":                       "2.*",
    "phpunit/phpunit":                  "~5.4",
    "platinumpixs/phing-random-string": "dev-master",
    "squizlabs/php_codesniffer":        "^2.4",
    "symfony/yaml":                     "~2.7",
    "drupal/coder":                     "~8.2",
    "symfony/console":                  "~2",
    "symfony/twig-bridge": "~2",
    "symfony/http-kernel": "~2",
    "symfony/twig-bundle": "~2",
    "symfony/framework-bundle": "~2"
  },
  "autoload": {
    "psr-4": {"Acquia\\Blt\\": "src/"},
    "classmap": [
      "app/AppKernel.php"
    ]
  },
  "bin": [
    "bin/blt",
    "bin/blt-console"
  ],
  "minimum-stability": "beta",
  "prefer-stable": true,
  "scripts": {
    "install-blt-alias": "./blt.sh install-alias"
  },
  "extra": {
    "symfony-app-dir": "app"
  }
}
